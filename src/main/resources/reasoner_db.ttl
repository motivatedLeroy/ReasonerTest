@prefix map: <#> .
@prefix db: <> .
@prefix vocab: <vocab/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix jdbc: <http://d2rq.org/terms/jdbc/> .

map:database a d2rq:Database;
	d2rq:jdbcDriver "org.postgresql.Driver";
	d2rq:jdbcDSN "jdbc:postgresql:///rdf_Reasoner";
	d2rq:username "postgres";
	d2rq:password "ines";
	.

# Table rdf_file
map:rdf_file a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "rdf_file/@@rdf_file.id@@";
	d2rq:class vocab:rdf_file;
	d2rq:classDefinitionLabel "rdf_file";
	.
map:rdf_file__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:rdf_file;
	d2rq:property rdfs:label;
	d2rq:pattern "rdf_file #@@rdf_file.id@@";
	.
map:rdf_file_id a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:rdf_file;
	d2rq:property vocab:rdf_file_id;
	d2rq:propertyDefinitionLabel "rdf_file id";
	d2rq:column "rdf_file.id";
	d2rq:datatype xsd:integer;
	.
map:rdf_file_date_of_upload a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:rdf_file;
	d2rq:property vocab:rdf_file_date_of_upload;
	d2rq:propertyDefinitionLabel "rdf_file date_of_upload";
	d2rq:column "rdf_file.date_of_upload";
	.
map:rdf_file_file_name a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:rdf_file;
	d2rq:property vocab:rdf_file_file_name;
	d2rq:propertyDefinitionLabel "rdf_file file_name";
	d2rq:column "rdf_file.file_name";
	.
map:rdf_file_number_of_entries a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:rdf_file;
	d2rq:property vocab:rdf_file_number_of_entries;
	d2rq:propertyDefinitionLabel "rdf_file number_of_entries";
	d2rq:column "rdf_file.number_of_entries";
	d2rq:datatype xsd:integer;
	.
map:rdf_file_provider a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:rdf_file;
	d2rq:property vocab:rdf_file_provider;
	d2rq:propertyDefinitionLabel "rdf_file provider";
	d2rq:column "rdf_file.provider";
	.
map:rdf_file_rating a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:rdf_file;
	d2rq:property vocab:rdf_file_rating;
	d2rq:propertyDefinitionLabel "rdf_file rating";
	d2rq:column "rdf_file.rating";
	d2rq:datatype xsd:double;
	.
map:rdf_file_tags a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:rdf_file;
	d2rq:property vocab:rdf_file_tags;
	d2rq:propertyDefinitionLabel "rdf_file tags";
	d2rq:column "rdf_file.tags";
	.
map:rdf_file_type a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:rdf_file;
	d2rq:property vocab:rdf_file_type;
	d2rq:propertyDefinitionLabel "rdf_file type";
	d2rq:column "rdf_file.type";
	.

